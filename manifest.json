{
  "manifest_version": 2,

  "name": "Godville Bingo God",
  "description": "This extension makes my bingo",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "https://godville.net/*"
      ],
      "js": ["godville_bingo.js"]
    }
  ],
  
  "browser_action": {
    "default_icon": "icon.png"
  }
}
